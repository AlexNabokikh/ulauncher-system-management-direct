{
  "manifest_version": "2",
  "required_api_version": "2",
  "api_version": "1",
  "name": "Power Manager",
  "description": "Lock, Suspend, Shut Down and Restart from the Ulauncher",
  "developer_name": "Alexander Nabokikh",
  "icon": "icon.svg",
  "preferences": [
    {
      "id": "lock-screen",
      "type": "keyword",
      "name": "Lock Screen",
      "default_value": "Lock Screen",
      "description": "Lock your screen"
    },
    {
      "id": "restart",
      "type": "keyword",
      "name": "Reboot / Restart",
      "default_value": "Reboot / Restart",
      "description": "Restart the system"
    },
    {
      "id": "suspend",
      "type": "keyword",
      "name": "Suspend / Sleep",
      "default_value": "Suspend / Sleep",
      "description": "Put the system to sleep"
    },
    {
      "id": "shutdown",
      "type": "keyword",
      "name": "Shut Down / Power Off",
      "default_value": "Shut Down / Power Off",
      "description": "Shut down system"
    }
  ]
}
